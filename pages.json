{
	"easycom": {},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "%tabBar.home%"
			}
		},
		{
			"path": "pages/mine/mine",
			"style": {
				"navigationBarTitleText": "%tabBar.mine%"
			}
		}
	],
	"subPackages": [{
		"root": "views",
		"pages": [{
			"path": "landscape/landscape"
		}]
	}],
	"tabBar": {
		"borderStyle": "white", // 微信小程序端仅支持 black/white
		"color": "#333",
		"selectedColor": "#333",
		"backgroundColor": "#F8F8F8",
		"list": [ // list 数组对应底部导航栏的顺序
			{
				"pagePath": "pages/index/index",
				"iconPath": "static/icons/home.png",
				"selectedIconPath": "static/icons/home-fill.png",
				"text": "%tabBar.home%"
			},
			{
				"pagePath": "pages/mine/mine",
				"iconPath": "static/icons/customer-bussinessman.png",
				"selectedIconPath": "static/icons/customer-businessman-fill.png",
				"text": "%tabBar.mine%"
			}
		]
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {}
}